local function QBSPEU_fake_script() -- TextButton_12.LocalScript 
	local script = Instance.new('LocalScript', TextButton_12)

	local button = script.Parent
	
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local RequestCommand = ReplicatedStorage
		:WaitForChild("HDAdminHDClient")
		:WaitForChild("Signals")
		:WaitForChild("RequestCommand")
	
	local running = false
	
	button.MouseButton1Click:Connect(function()
		running = not running
	
		while running do
			RequestCommand:InvokeServer(";place all 116715518379271")
			task.wait(1)
		end
	end)
	
end
coroutine.wrap(QBSPEU_fake_script)()
